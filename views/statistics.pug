h4 Users
table.table
  thead
    tr
      th User ID
      th Questions Attempted
      th Correct Answers
      th Success Rate
      th Points
  tbody
    each s in students
      tr
        td #{s.id}
        td #{s.attemptedCount}
        td #{s.answeredCount}
        if (s.answeredCount + s.attemptedCount) === 0
          td 0%
        else
          td #{((s.answeredCount / (s.answeredCount + s.attemptedCount)) * 100).toFixed(2)}%
        td #{s.points}

h4 Questions
table.table
  thead
    tr
      th ID
      th Title
      th Points
      th Total Attempts
      th Correct Answers
      th Success Rate
      th First Answer
  tbody
    each q in questions
      tr
        td #{q.id}
        td #{q.title}
        td #{q.points}
        td #{q.totalCount}
        td #{q.answeredCount}
        if q.totalCount === 0
          td 0%
        else
          td #{((q.answeredCount / q.totalCount) * 100).toFixed(2)}%
        td #{q.firstAnswer}

