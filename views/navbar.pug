.navbar-fixed
  nav
    .nav-wrapper
      a.navbarLink.brand-logo.center(href='#!') Quizzard
      a.navbarLink.button-collapse(href='#', data-activates='mobile-demo')
        i.material-icons menu
      ul.left.hide-on-med-and-down
        if user.type == 0
          li#nav-home
            a.navbarLink(href='/admin') Administration
        else
          li#nav-home
            a.navbarLink(href='/home') Questions
        li#nav-leaderboard
          a.navbarLink(href='/leaderboard') Leaderboard
        li#nav-about
          a.navbarLink(href='/about') About
      ul.right.hide-on-med-and-down
        li#nav-profile
          a.navbarLink(href='#') Profile
        li#nav-logout
          a.navbarLink(href='#') Logout
      ul#mobile-demo.side-nav
        if user.type == 0
          li#nav-home
            a.navbarLink(href='/admin') Administration
        else
          li#nav-home
            a.navbarLinkHidden(href='/home') Questions
        li#nav-leaderboard
          a.navbarLinkHidden(href='/leaderboard') Leaderboard
        li#nav-about
          a.navbarLinkHidden(href='/about') About
        li#nav-profile
          a.navbarLinkHidden(href='#') Profile
        li#nav-logout
          a.navbarLinkHidden(href='#') Logout